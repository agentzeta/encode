{
  "name": "oev-dashboard",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "shadcn",
    "url": "https://twitter.com/shadcn"
  },
  "workspaces": [
    "apps/*",
    "packages/*",
    "templates/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "turbo run build",
    "build:cli": "turbo --filter=shadcn build",
    "build:registry": "pnpm --filter=www build:registry && pnpm format:write -- --loglevel silent",
    "registry:build": "pnpm --filter=www build:registry && pnpm format:write -- --loglevel silent",
    "registry:capture": "pnpm --filter=www registry:capture",
    "cli:dev": "turbo --filter=shadcn-ui dev",
    "cli:start": "pnpm --filter=shadcn-ui start:dev",
    "cli:build": "pnpm --filter=shadcn-ui build",
    "shadcn:dev": "turbo --filter=shadcn dev",
    "shadcn": "pnpm --filter=shadcn start:dev",
    "shadcn:build": "pnpm --filter=shadcn build",
    "shadcn:test": "pnpm --filter=shadcn test",
    "docs:build": "pnpm --filter=www build:docs",
    "www:dev": "pnpm --filter=www dev",
    "www:build": "pnpm --filter=www build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "preview": "turbo run preview",
    "typecheck": "turbo run typecheck",
    "format:write": "turbo run format:write",
    "format:check": "turbo run format:check",
    "sync:templates": "./scripts/sync-templates.sh \"templates/*\"",
    "check": "turbo lint typecheck format:check",
    "release": "changeset version",
    "pub:beta": "cd packages/shadcn && pnpm pub:beta",
    "pub:release": "cd packages/shadcn && pnpm pub:release",
    "test": "turbo run test --force"
  },
  "packageManager": "pnpm@9.0.6",
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@hookform/resolvers": "^3.9.0",
    "@ianvs/prettier-plugin-sort-imports": "^3.7.2",
    "@manypkg/cli": "^0.20.0",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.1",
    "@radix-ui/react-context-menu": "^2.2.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-hover-card": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.2",
    "@radix-ui/react-navigation-menu": "^1.2.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.0",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.3",
    "@tanstack/react-table": "^8.9.1",
    "@typescript-eslint/parser": "^5.59.7",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "concurrently": "^8.0.1",
    "cross-env": "^7.0.3",
    "date-fns": "^3.0.0",
    "embla-carousel-react": "^8.3.0",
    "eslint": "^8.41.0",
    "eslint-config-next": "13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-turbo": "^1.9.9",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "3.13.1",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.453.0",
    "next-themes": "^0.3.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "puppeteer": "^23.6.0",
    "react-day-picker": "8.10.1",
    "react-hook-form": "^7.53.1",
    "react-resizable-panels": "^2.1.5",
    "recharts": "^2.13.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "3.4.6",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.1.4",
    "turbo": "^1.9.9",
    "vaul": "^1.1.0",
    "vite": "^5.4.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^2.0.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.27",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "typescript": "^5.5.3"
  }
}
